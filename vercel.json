{
  "rewrites": [
    {
      "source": "/yono",
      "destination": "/index.html"
    },
    {
      "source": "/yono/",
      "destination": "/index.html"
    },
    {
      "source": "/yono/about-us",
      "destination": "/about-us.html"
    },
    {
      "source": "/yono/privacy-policy",
      "destination": "/privacy-policy.html"
    },
    {
      "source": "/yono/disclaimer",
      "destination": "/disclaimer.html"
    },
    {
      "source": "/yono/resources/:path*",
      "destination": "/resources/:path*"
    },
    {
      "source": "/yono/style.css",
      "destination": "/style.css"
    },
    {
      "source": "/yono/script.js",
      "destination": "/script.js"
    },
    {
      "source": "/yono/robots.txt",
      "destination": "/robots.txt"
    },
    {
      "source": "/yono/sitemap.xml",
      "destination": "/sitemap.xml"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}

